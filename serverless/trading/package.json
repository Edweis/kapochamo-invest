{
  "name": "kapochamo-invest",
  "version": "1.0.1",
  "main": "src/handler.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --ext ts,js --no-ignore --fix",
    "serve:watch": "nodemon -e js,ts,jsx,tsx -x npm run serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "compile": "tsc && node app.js"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/cheerio": "^0.22.17",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/lodash": "^4.14.149",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^12.7.5",
    "@types/nunjucks": "^3.1.3",
    "@types/pg": "^7.14.3",
    "@types/puppeteer": "^2.0.1",
    "@types/puppeteer-core": "^2.0.0",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.2.0",
    "aws-sdk": "^2.673.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "html-loader": "^1.1.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-environment-puppeteer": "^4.4.0",
    "jest-mock-axios": "^4.0.0",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.19.2",
    "prettier": "^1.19.1",
    "puppeteer": "^3.1.0",
    "serverless": "^1.70.0",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-prune-plugin": "^1.4.3",
    "serverless-webpack": "^5.3.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.1.0",
    "typescript": "^3.9.3",
    "webpack": "^4.39.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.3",
    "chrome-aws-lambda": "^3.0.4",
    "dotenv": "^8.2.0",
    "http-status-codes": "^1.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.28",
    "nunjucks": "^3.2.1",
    "p-limit": "^2.3.0",
    "pg": "^8.0.2",
    "puppeteer-core": "^3.0.4",
    "ws": "^7.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh clean_notebook.sh && lint-staged && npm test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "(.*\\.test\\.(tsx?|jsx?))$",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/src/teardown.ts"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  }
}
